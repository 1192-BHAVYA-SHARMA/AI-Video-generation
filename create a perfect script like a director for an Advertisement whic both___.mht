@charset "utf-8";

@keyframes snow-anim { 
  0% { top: -10%; }
  100% { top: 110%; }
}

@keyframes snow-anim-wave { 
  0%, 50%, 100% { transform: translateX(0px); }
  30% { transform: translateX(4px); }
  60% { transform: translateX(-2px); }
}

.animation-weather-snow { animation: 3s linear 0s infinite normal none running snow-anim; }

.animation-weather-snow-wave { animation: 3s cubic-bezier(0.45, 0, 0.55, 1) 0s infinite normal none running snow-anim-wave; }

@keyframes star-twinkle { 
  0%, 60%, 100% { opacity: 1; }
  80% { opacity: 0.1; }
}

@keyframes star-shoot { 
  0% { left: 0px; top: 60%; opacity: 0; }
  2% { opacity: 1; }
  8% { left: 100%; top: 40%; opacity: 0.6; }
  9% { opacity: 0; }
  100% { left: 100%; top: 40%; opacity: 0; }
}

.animation-weather-star-twinkle { animation: 1s ease 0s infinite normal none running star-twinkle; }

.animation-weather-star-shoot { animation: 12s linear 2s infinite normal none running star-shoot; transform: rotate(-10deg); mask-image: linear-gradient(270deg, black, transparent); opacity: 0; }

@media (min-width: 640px) {
  .animation-weather-star-shoot { transform: rotate(-6deg); }
}

@keyframes rain-anim { 
  0% { top: -10%; }
  100% { top: 110%; }
}

@keyframes rain-splash { 
  0%, 90% { opacity: 0; }
  100% { opacity: 1; }
}

.animation-weather-rain { animation: 1s ease 0s infinite normal none running rain-anim; }

.animation-weather-rain-splash { animation: 1s ease 0s infinite normal none running rain-splash; }

.shimmer { animation-name: shimmer; animation-iteration-count: infinite; mask-image: linear-gradient(-70deg, black 50%, rgba(0, 0, 0, 0.333), black 65%); mask-size: 300% 100%; mask-repeat: no-repeat; mask-position: right center; }

@media (min-width: 640px) {
  .shimmer { mask-image: linear-gradient(-70deg, black 35%, rgba(0, 0, 0, 0.333), black 55%); }
}

@keyframes shimmer { 
  100% { mask-position: left center; }
}

.dotGridContainer { --dot-bg: oklch(var(--background-color-100)/1); --dog-bg-highlight: oklch(var(--background-super-color-200)/1); --dot-color: transparent; --dot-size: 3px; --dot-space: 22px; }

.dotGrid { background-image: ; background-size: ; background-repeat: ; background-attachment: ; background-origin: ; background-clip: ; background-color: ; background-position: 0px 0px; }

.dotHighlightGrid { display: grid; grid-auto-columns: var(--dot-space); grid-auto-rows: var(--dot-space); }

.gridDot { display: flex; align-items: center; justify-content: center; }

.gridDot::before { content: ""; width: 100%; height: 100%; animation: 2s ease 0s 1 normal both running gridDotFade; background-color: currentcolor; }

.gridDot.highlight::before { background-color: var(--dog-bg-highlight); }

@keyframes gridDotFade { 
  0% { opacity: 0; }
  20% { opacity: 1; }
  100% { opacity: 0; }
}

.wrapper { perspective: 1000px; position: relative; }

.shadow { width: 80%; height: 3px; background: black; position: absolute; border-radius: 50%; filter: blur(2px); opacity: 0.05; top: calc(100% + 3px); left: 50%; transform: translateX(-50%); }

.dark .shadow { opacity: 0.4; }

.coin { height: 100%; width: 100%; position: absolute; transform-style: preserve-3d; transform-origin: center center; border-radius: 50%; }

.coin.animate { animation-name: coin-spin; }

.coin .back, .coin .front { position: absolute; height: 100%; width: 100%; border-radius: 50%; background-size: cover; }

.coin .side, .coin .side .spoke { transform-style: preserve-3d; backface-visibility: hidden; }

.coin .side .spoke { height: 100%; width: 100%; position: absolute; }

.coin .side .spoke .facet { content: ""; display: block; position: absolute; background: rgb(187, 187, 187); }

.dark .coin .side .spoke .facet { background: rgb(170, 170, 170); }

.coin .side .spoke .facet:first-child { transform-origin: center top; }

.coin .side .spoke .facet:last-child { bottom: 0px; transform-origin: center bottom; }

.coin .bottom, .coin .top { position: absolute; border-radius: 50%; height: 100%; width: 100%; }

.animate-maintenance { animation: 40s linear 0s infinite normal forwards running maintenanceLoader; stroke-dasharray: 20, 16; stroke-dashoffset: 477.303; }

@keyframes maintenanceLoader { 
  100% { stroke-dashoffset: 0; }
}

.gradient-blur { width: 100%; pointer-events: none; bottom: 0px; position: absolute; }

.gradient-blur::after, .gradient-blur::before, .gradient-blur > div { position: absolute; inset: 0px; }

.gradient-blur::before { content: ""; z-index: 1; backdrop-filter: blur(0.2px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 0px, rgb(0, 0, 0) 12.5%, rgb(0, 0, 0) 25%, rgba(0, 0, 0, 0) 37.5%); }

.gradient-blur > div:first-of-type { z-index: 2; backdrop-filter: blur(0.3px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 12.5%, rgb(0, 0, 0) 25%, rgb(0, 0, 0) 37.5%, rgba(0, 0, 0, 0) 50%); }

.gradient-blur > div:nth-of-type(2) { z-index: 3; backdrop-filter: blur(0.4px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 25%, rgb(0, 0, 0) 37.5%, rgb(0, 0, 0) 50%, rgba(0, 0, 0, 0) 62.5%); }

.gradient-blur > div:nth-of-type(3) { z-index: 4; backdrop-filter: blur(1px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 37.5%, rgb(0, 0, 0) 50%, rgb(0, 0, 0) 62.5%, rgba(0, 0, 0, 0) 75%); }

.gradient-blur > div:nth-of-type(4) { z-index: 5; backdrop-filter: blur(1px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 50%, rgb(0, 0, 0) 62.5%, rgb(0, 0, 0) 75%, rgba(0, 0, 0, 0) 87.5%); }

.gradient-blur > div:nth-of-type(5) { z-index: 6; backdrop-filter: blur(2px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 62.5%, rgb(0, 0, 0) 75%, rgb(0, 0, 0) 87.5%, rgba(0, 0, 0, 0)); }

.gradient-blur > div:nth-of-type(6) { z-index: 7; backdrop-filter: blur(2px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 75%, rgb(0, 0, 0) 87.5%, rgb(0, 0, 0)); }

.gradient-blur::after { content: ""; z-index: 8; backdrop-filter: blur(3px); mask: linear-gradient(0deg, rgba(0, 0, 0, 0) 87.5%, rgb(0, 0, 0)); }

.article-prose .prose { font-size: 18px; line-height: 1.6; }

.markdown-highlight { background-color: rgba(32, 128, 141, 0.5); }

.dark .markdown-highlight { background-color: rgba(31, 184, 205, 0.1); color: rgb(31, 184, 205); }

@media print {
  h1, h2, h3, h4, h5, h6 { break-after: avoid; }
  h1, h2, h3, h4, h5, h6, ol, ul { break-inside: avoid; }
  .scrollable-container { height: auto !important; overflow: visible !important; max-height: none !important; border: none !important; }
